                            <scriptNote ana="ink" n="Überschrift">
                                <p><locus n="fol. 3r, fol. 33r, fol. 58v, fol. 88r"/>Nicht-vitriolische Eisengallustinten</p></scriptNote>
                            <scriptNote ana="ink" n="Konkordanz">
